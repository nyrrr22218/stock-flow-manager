generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model item_name {
  id        BigInt   @id(map: "itemName_pkey") @default(autoincrement())
  item_name String   @unique(map: "itemname_itemname_key")
  order     order?
  product   product?
  stock     stock?
}

model logs {
  id          BigInt   @id(map: "log_pkey") @default(autoincrement())
  log_message String
  logged_at   DateTime @default(now()) @db.Timestamptz(6)
}

model order {
  id           BigInt    @id @default(autoincrement())
  order_count  Int?
  item_name_id BigInt    @unique(map: "order_itemname_id_key")
  ordered_at   DateTime  @default(now()) @db.Timestamptz(6)
  item_name    item_name @relation(fields: [item_name_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model product {
  id              BigInt    @id @default(autoincrement())
  producted_count Int
  item_name_id    BigInt    @unique(map: "product_itemname_id_key")
  produced_at     DateTime  @default(now()) @db.Timestamptz(6)
  item_name       item_name @relation(fields: [item_name_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model shipments {
  id             BigInt   @id @default(autoincrement())
  order_snapshot Json     @db.Json
  created_at     DateTime @default(now()) @db.Timestamptz(6)
}

model stock {
  id           BigInt    @id @default(autoincrement())
  stock_count  Int
  item_name_id BigInt    @unique(map: "stock_itemname_id_key")
  item_name    item_name @relation(fields: [item_name_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
